@model IEnumerable<Garage_ManagementFinal.order_service>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<h2>View All Repaired Service</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<hr />
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.quantity)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Assistant_Mechanic)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Entry_Form.plate_no)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.service_table.service_name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.quantity)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Assistant_Mechanic)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Entry_Form.plate_no)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.service_table.service_name)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id = item.Id }, new { @class = "btn btn-info" }) 
          
        </td>
    </tr>
}

</table>
